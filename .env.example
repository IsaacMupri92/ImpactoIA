# Configuración de ejemplo para ImpactoIA
# Copia este archivo a .env y ajusta los valores según necesites

# Modo debug (true/false)
IMPACTO_IA_DEBUG=false

# Nivel de logging (DEBUG, INFO, WARNING, ERROR)
IMPACTO_IA_LOG_LEVEL=INFO

# Umbral de impacto significativo (0-100)
IMPACTO_IA_UMBRAL_IMPACTO=50.0

# Directorio de datos (opcional)
# IMPACTO_IA_DATA_DIR=/path/to/data
